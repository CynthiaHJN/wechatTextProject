.absoluteBg {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 220rpx;
  background: linear-gradient(#fe8543, #ffe786);
}
.typeList {
  width: 100%;
  z-index: 2;
  padding-top: 30rpx;
}
.typeList .typeItem{
  position: relative;
  width: 93%;
  margin: 0 auto 20rpx;
  height: 340rpx;
  background: #ddd;
  border-radius: 15rpx;
}
.typeList .typeItem .typeNavigate {
  position: absolute;
  width: 100%;
  height: 140rpx;
  background: rgba(255,255,255,.7);
  top: 100rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
}
.typeList .typeItem .typeNavigate .typeName {
  font-size: 18px;
  text-align: center;
  width: 100%;
  display: block;
  margin-bottom: 20rpx;
  color: #666;
}
.typeList .typeItem .typeNavigate  .typeButton {
  background-size: cover;
  background-color: white;
  width: 160rpx;
  font-size: 12px;
  height: 40rpx;
  line-height: 36rpx;
  border: 1px solid #666;
  color: #666;
  background: transparent;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.typeList .typeItem .typeNavigate  .typeButton:after {
  border: none;
}
.bottomTip {
  display: block;
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
  color: #ddd;
  text-align: center;
}